# Ignore bundler config.
.bundle
vendor

# Ignore the default SQLite database.
*.sqlite3
*.sqlite3-journal

# Ignore Mac OS X files.
.DS_Store

# Ignore log files and temp files.
log/*
tmp/*
!/log/.keep
!/tmp/.keep

# Ignore coverage reports generated by SimpleCov.
coverage/

# Ignore secret files.
config/secrets.yml
.env


# Ignore Yarn lock file (if you prefer using Yarn).
yarn.lock

# Ignore compiled binary files.
*.o
*.out
*.exe

# Ignore Byebug command history file.
.byebug_history

# Ignore various other editor and IDE files.
.idea/
.vscode/
*.swp
*~

# Ignore the `vendor/bundle` directory.
vendor/bundle/

# Ignore the `.env` file, which is used to set environment variables.
.env

# Ignore gems that were installed locally.
vendor/cache/


